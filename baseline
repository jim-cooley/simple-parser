logpath="./etc/log"
bslpath="./etc/bsl"

for test in ./etc/log/$1; do
    baseline="$bslpath"/"$(basename $test)"
    printf "$(basename $test):  \m"
    if [ -f "$baseline" ]; then
        result="fail"
        diff "$baseline" "$test" && result="pass"
        printf "$(basename $test):  $result\n"
    else
        printf "no baseline\n"
    fi
done