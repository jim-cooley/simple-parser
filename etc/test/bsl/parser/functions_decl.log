x => x:x + 1

sugar(x) => { x + 1 }

sugar(x) := { x + 1 }

sugar(x) = { x + 1 }

def simple(x) := {
    x + 1
}

def factorial(x) := {
    if x == 1 then
        return 1
    else
        x * factorial(x - 1)
}

# simple inline functions
a := 3
a := a + 1
a => a + 1

# inline functions:
def atr = (high + low) / 2
def avg_daily_price = (open + close) / 2
def trade_size = position * 10%
